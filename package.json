{
  "name": "flutter-dev-tools",
  "displayName": "FlutterDevTools",
  "description": "flutter开发工具集",
  "publisher": "wanggaowan",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.99.0"
  },
  "extensionKind": [
    "workspace"
  ],
  "capabilities": {
    "virtualWorkspaces": {
      "supported": false
    },
    "untrustedWorkspaces": {
      "supported": false
    }
  },
  "categories": [
    "Other"
  ],
  "license": "SEE LICENSE IN LICENSE",
  "repository": {
    "type": "git",
    "url": "https://github.com/wanggaowan/flutter-dev-tools"
  },
  "keywords": [
    "dart",
    "flutter"
  ],
  "icon": "media/flutter.png",
  "activationEvents": [
    "workspaceContains:pubspec.yaml",
    "workspaceContains:*/pubspec.yaml",
    "workspaceContains:*/*/pubspec.yaml",
    "workspaceContains:*.dart",
    "workspaceContains:*/*.dart",
    "workspaceContains:*/*/*.dart"
  ],
  "extensionDependencies": [
    "Dart-Code.dart-code"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "flutterProjectView.addFile",
        "title": "新建文件",
        "icon": "$(file-add)"
      },
      {
        "command": "flutterProjectView.addFolder",
        "title": "新建文件夹",
        "icon": "$(file-directory-create)"
      },
      {
        "command": "flutterProjectView.rename",
        "title": "重命名"
      },
      {
        "command": "flutterProjectView.delete",
        "title": "删除",
        "icon": "$(trash)"
      },
      {
        "command": "flutterProjectView.refresh",
        "title": "刷新",
        "icon": "$(refresh)"
      },
      {
        "command": "flutterDevTools.json2dart",
        "title": "json to dart"
      },
      {
        "command": "flutterDevTools.genConstructor",
        "title": "创建构造函数"
      },
      {
        "command": "flutterDevTools.genSerialization",
        "title": "创建序列化方法"
      }
    ],
    "submenus": [
      {
        "id": "flutterDevTools.editorContext",
        "label": "Flutter"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "flutterProjectView.addFile",
          "when": "false"
        },
        {
          "command": "flutterProjectView.addFolder",
          "when": "false"
        },
        {
          "command": "flutterProjectView.refresh",
          "when": "false"
        },
        {
          "command": "flutterProjectView.delete",
          "when": "false"
        },
        {
          "command": "flutterProjectView.rename",
          "when": "false"
        },
        {
          "command": "flutterDevTools.json2dart",
          "when": "false"
        },
        {
          "command": "flutterDevTools.genConstructor",
          "when": "false"
        },
        {
          "command": "flutterDevTools.genSerialization",
          "when": "false"
        }
      ],
      "view/title": [
        {
          "command": "flutterProjectView.addFile",
          "when": "view == flutterProjectView",
          "group": "navigation@1"
        },
        {
          "command": "flutterProjectView.addFolder",
          "when": "view == flutterProjectView",
          "group": "navigation@2"
        },
        {
          "command": "flutterProjectView.refresh",
          "when": "view == flutterProjectView",
          "group": "navigation@3"
        }
      ],
      "view/item/context": [
        {
          "command": "flutterProjectView.addFile",
          "when": "view == flutterProjectView",
          "group": "fileOp@1"
        },
        {
          "command": "flutterProjectView.addFolder",
          "when": "view == flutterProjectView",
          "group": "fileOp@2"
        },
        {
          "command": "flutterProjectView.rename",
          "when": "view == flutterProjectView",
          "group": "fileOp@3"
        },
        {
          "command": "flutterProjectView.delete",
          "when": "view == flutterProjectView",
          "group": "fileOp@4"
        }
      ],
      "editor/context": [
        {
          "submenu": "flutterDevTools.editorContext",
          "when": "flutterDevTools.isFlutterProject"
        }
      ],
      "flutterDevTools.editorContext": [
        {
          "command": "flutterDevTools.json2dart",
          "when": "resourceLangId == dart && flutterDevTools.isFlutterProject",
          "group": "flutterDevTools"
        },
        {
          "command": "flutterDevTools.genConstructor",
          "when": "resourceLangId == dart && flutterDevTools.isFlutterProject",
          "group": "flutterDevTools@2"
        },
        {
          "command": "flutterDevTools.genSerialization",
          "when": "resourceLangId == dart && flutterDevTools.isFlutterProject",
          "group": "flutterDevTools@3"
        }
      ]
    },
    "views": {
      "explorer": [
        {
          "id": "flutterProjectView",
          "name": "Flutter",
          "icon": "media/flutter.svg",
          "when": "flutterDevTools.isFlutterProject"
        }
      ],
      "resource-manger": [
        {
          "id": "imagePreviewView",
          "type": "webview",
          "name": "Image",
          "icon": "media/resource.svg",
          "when": "flutterDevTools.isFlutterProject"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "resource-manger",
          "title": "Resource Manager",
          "icon": "media/Resources.svg"
        }
      ]
    },
    "configuration": [
      {
        "title": "资源配置",
        "properties": {
          "flutterDevTools.imageSrcPath": {
            "type": "string",
            "default": "assets/images",
            "description": "存放图片资源的目录,此目录为基于Flutter项目根目录的相对目录",
            "scope": "window"
          }
        }
      },
      {
        "title": "json to dart",
        "properties": {
          "flutterDevTools.classSuffix": {
            "type": "string",
            "default": "Entity",
            "description": "生成的类后缀",
            "scope": "window"
          },
          "flutterDevTools.generateDoc": {
            "type": "boolean",
            "default": true,
            "description": "json值是否作为备注",
            "scope": "window"
          }
        }
      },
      {
        "title": "class生成配置",
        "properties": {
          "flutterDevTools.converts": {
            "type": "string",
            "default": "",
            "description": "生成序列化方法时，@JsonSerializable(converters: xxx)中converters名称",
            "scope": "window"
          }
        }
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "dependencies": {
    "yaml": "^2.7.0",
    "vscode-uri": "^3.1.0"
  },
  "devDependencies": {
    "@types/vscode": "^1.99.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "eslint": "^9.23.0",
    "esbuild": "^0.25.1",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.8.2",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
